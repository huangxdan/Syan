<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/black"
    android:paddingBottom="@dimen/default_20dp_hf"
    tools:context=".syan.SYanActivity">

    <include
        layout="@layout/public_navigation_bar_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/default_0dp_hf"
        android:layout_weight="1"
        android:layout_marginTop="@dimen/default_20dp_hf"
        android:scaleType="centerInside"
        android:src="@drawable/pic_default" />

    <View
        android:layout_width="wrap_content"
        android:layout_height="@dimen/default_20dp_hf"  />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/default_0dp_hf"
        android:layout_weight="1"
        android:scaleType="centerInside"
        android:src="@drawable/pic_default" />

</LinearLayout>